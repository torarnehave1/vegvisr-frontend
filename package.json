{
  "name": "vegvisr-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vue": "vite",
    "dev:m-worker": "wrangler dev --config main-worker/wrangler.toml --port=8787",
    "dev:d-worker": "wrangler dev --config dev-worker/wrangler.toml --port=8788",
    "dev:api-worker": "wrangler dev --config api-worker/wrangler.toml --port=8789",
    "dev": "concurrently \"npm run dev:vue\" \"npm run dev:m-worker\" \"npm run dev:d-worker\" \"npm run dev:api-worker\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "dotenv": "^16.4.7",
    "hono": "^4.7.4",
    "openai": "^4.87.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "wrangler": "^4.0.0"
  },
  "engines": {
    "node": "20.11.1"
  }
}
