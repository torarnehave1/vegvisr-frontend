{
  "name": "vegvisr-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vue": "vite",
    "dev:m-worker": "wrangler dev --config main-worker/wrangler.toml --port=8787 --remote",
    "dev:d-worker": "wrangler dev --config dev-worker/wrangler.toml --port=8788",
    "dev:api-worker": "wrangler dev --config api-worker/wrangler.toml --port=8789",
    "dev": "concurrently \"npm run dev:vue\" \"npm run dev:m-worker\" \"npm run dev:d-worker\" \"npm run dev:api-worker\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "ai": "^4.3.15",
    "axios": "^1.8.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "codemirror": "^6.0.1",
    "crypto": "^1.0.1",
    "cytoscape": "^3.31.2",
    "cytoscape-undo-redo": "^1.3.3",
    "dotenv": "^16.4.7",
    "hono": "^4.7.4",
    "html-to-pdfmake": "^2.5.26",
    "html2pdf.js": "^0.10.3",
    "jose": "^6.0.10",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "marked": "^15.0.7",
    "mermaid": "^11.6.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.87.3",
    "pdfmake": "^0.2.20",
    "pinia": "^3.0.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-markdown-it": "^1.0.10",
    "workers-ai-provider": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "browserify-fs": "^1.0.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.1",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "3.5.3",
    "url": "^0.11.4",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "wrangler": "^4.20.5"
  },
  "engines": {
    "node": "20.11.1"
  }
}
